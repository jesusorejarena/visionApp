{
  "name": "visionApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --interactive",
    "pods": "cd ios && bundle install && bundle exec pod install",
    "pods-intel": "cd ios && pod install",
    "m-clean": "npm cache clean --force && watchman watch-del-all && cd android && ./gradlew clean",
    "install-all": "npm i && npm run pods && npm run m-clean",
    "install-all-intel": "npm i && npm run pods-intel && npm run m-clean",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-camera-roll/camera-roll": "^7.6.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "axios": "^1.6.8",
    "lottie-react-native": "^6.7.2",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.74.0",
    "react-native-awesome-gallery": "^0.3.9",
    "react-native-gesture-handler": "^2.16.0",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "^3.9.0",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "^15.2.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-vision-camera": "^4.0.1",
    "react-native-background-upload": "github:cristianoccazinsp/react-native-background-upload"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.81",
    "@react-native/eslint-config": "0.74.81",
    "@react-native/metro-config": "0.74.81",
    "@react-native/typescript-config": "0.74.81",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.3.0",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
